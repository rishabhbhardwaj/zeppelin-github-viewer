{
  "manifest_version": 2,

  "name": "Apache Zeppelin Github Viwer",
  "version": "0.0.1",
  "description": "Github Viewer for Apache Zeppelin",

  "content_scripts": [{
    "css": ["style.css"],
    "js": ["jquery-3.2.1.min.js","content.js"],
    "matches": ["https://*.github.com/*/blob/*/*"]
  }]
}
