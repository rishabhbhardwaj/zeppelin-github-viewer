{
  "manifest_version": 2,

  "name": "Apache Zeppelin Github Viwer",
  "version": "0.0.1",
  "description": "Github Viewer for Apache Zeppelin",

  "content_scripts": [{
    "css": ["css/style.css","css/default.min.css", "css/material.indigo-pink.min.css"],
    "js": ["js/jquery-3.2.1.min.js","content.js","js/material.min.js"],
    "matches": ["https://*.github.com/*/blob/*/*"],
    "run_at": "document_end"

  }]
}
